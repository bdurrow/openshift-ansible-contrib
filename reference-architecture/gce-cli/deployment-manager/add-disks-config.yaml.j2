imports:
- path: add-disks.jinja
resources:
- name: add-disks
  type: add-disks.jinja
  properties:
    prefix: {{ prefix }}
    node_docker_disk_size: {{ node_docker_disk_size }}
    node_openshift_disk_size: {{ node_openshift_disk_size }}
    docker_instances:
      {{ docker_instances | to_yaml | indent(6) }}
    schedulable_instances:
      {{ schedulable_instances | to_yaml | indent(6) }}
